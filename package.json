{
  "name": "polyai",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "clear && ts-node index.ts",
    "dev": "clear && nodemon --watch '**/*.ts' --exec 'clear && ts-node' index.ts",
    "load": "sudo insmod polyai-reader/polyai.ko",
    "unload": "sudo rmmod polyai-reader/polyai.ko",
    "scan": "bash scan.sh",
    "check": "sudo dmesg | tail -n 5",
    "make": "cd polyai-reader && make",
    "pid": "pgrep -f Polytopia.exe",
    "compile": "g++ -o polyai-reader/polyai-reader polyai-reader/polyai-reader.cpp polyai-reader/reader_util.cpp -fvisibility=hidden -s -static -O2 && bash scan.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "better-sqlite3": "^11.10.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/express": "^5.0.0",
    "@types/node": "^22.18.1",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
